{
  "manifest_version": 3,
  "name": "Swifli Extension",
  "version": "1.0.0",
  "action": { "default_popup": "index.html" },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["src/content.jsx"],
      "run": "document_end"
    }
  ],
  "permissions": [
    "scripting",
    "activeTab"
  ]
}
